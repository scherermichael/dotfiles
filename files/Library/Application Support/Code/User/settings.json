{
  "AllAutocomplete.cycleOpenDocumentsOnLaunch": true,
  "AllAutocomplete.updateOnlyOnSave": true,
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[markdown]": {
    "editor.quickSuggestions": false,
    "editor.wordWrap": "on"
  },
  "[plaintext]": {
    "editor.quickSuggestions": false
  },
  "[yaml]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "none",
    "editor.quickSuggestions": false,
  },
  "[json]": {
    "editor.formatOnSave": false
  },
  "atomKeymap.promptV3Features": true,
  "breadcrumbs.enabled": false,
  "dayNightThemeSwitcher.dayTheme": "Atom One Light",
  "dayNightThemeSwitcher.nightTheme": "Atomize",
  "debug.console.fontFamily": "'SF Mono', Menlo, Monaco, 'Courier New', monospace",
  "debug.console.fontSize": 14,
  "debug.toolBarLocation": "docked",
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": false,
  "disableLigatures.mode": "Line",
  "editor.fontFamily": "'FiraCode-Retina', 'SF Mono', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": "'ss01', 'ss03'",
  "editor.fontSize": 14,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.hideCursorInOverviewRuler": true,
  "editor.hover.delay": 1000,
  "editor.hover.sticky": false,
  "editor.matchBrackets": "near",
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.occurrencesHighlight": false,
  "editor.parameterHints.cycle": false,
  "editor.quickSuggestions": false,
  "editor.roundedSelection": false,
  "editor.scrollBeyondLastLine": false,
  "editor.semanticHighlighting.enabled": false,
  "editor.suggest.showColors": false,
  "editor.suggest.showIcons": false,
  "editor.suggestOnTriggerCharacters": false,
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "Uncolored",
        "scope": [
          "entity.name",
          "meta.block variable.other",
          "string constant.other.placeholder",
          "string.interpolated.dollar.shell",
          "string.other.link",
          "support.other.variable",
          "variable.language",
          "variable.other.constant.js",
          "variable.parameter",
          "variable"
        ],
        "settings": {
          "foreground": "#ABB2BF"
        }
      },
      {
        "name": "Yellow Colored",
        "scope": [
          "punctuation.definition.anchor.yaml",
          "punctuation.definition.alias.yaml",
          "variable.other.alias.yaml"
        ],
        "settings": {
          "foreground": "#E5C07B"
        }
      },
      {
        "name": "Red Colored",
        "scope": [
          "constant.language.boolean.yaml",
          "entity.name.tag.yaml",
          "meta.object-literal.key",
          "punctuation.definition.template-expression",
          "punctuation.definition.variable.shell",
          "support.variable",
          "variable.other.bracket.shell",
          "variable.other.property"
        ],
        "settings": {
          "foreground": "#E06C75"
        }
      }
    ]
  },
  "emmet.showExpandedAbbreviation": "never",
  "eslint.format.enable": true,
  "explorer.openEditors.visible": 0,
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "onFocusChange",
  "files.trimFinalNewlines": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.postCommitCommand": "sync",
  "gitlens.blame.avatars": false,
  "gitlens.blame.heatmap.enabled": false,
  "gitlens.codeLens.enabled": true,
  "gitlens.codeLens.scopes": [
    "document"
  ],
  "gitlens.currentLine.enabled": false,
